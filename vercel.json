{
    "version": 3,
    "builds": [
      {
        "src": "chatApp/asgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
      },
      {
        "src": "manage.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/ws/socket-server/(?<chatroom_name>[^/]+)",
        "dest": "chatApp/asgi.py"
      },
      {
        "src": "/.*",
        "dest": "chatApp/wsgi.py"
      }
    ]
  }
  