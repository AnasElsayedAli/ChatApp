{
    "version": 2,
    "builds": [
      {
        "src": "manage.py",
        "use": "@vercel/python"
      },
      {
        "src": "chatApp/asgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/manage.py"
      },
      {
        "src": "/ws/socket-server/(.*)",
        "dest": "/chatApp/asgi.py"
      }
    ]
  }
  
  